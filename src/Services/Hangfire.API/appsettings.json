{
  "DatabaseSettings": {
    "DBProvider": "mongodb",
    "ConnectionString": "mongodb://admin:admin@localhost:27018/hangfiredb?authSource=admin"
  },
  "HangFireSettings": {
    "Route": "/jobs",
    "ServerName": "Customer API",
    "Dashboard": {
      "AppPath": "/",
