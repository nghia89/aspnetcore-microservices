      "DashboardTitle": "Jobs"
    },
    "Storage": {
      "DBProvider": "mongodb",
      "ConnectionString": "mongodb://admin:admin@localhost:27018/hangfiredb?authSource=admin"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Debug",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
